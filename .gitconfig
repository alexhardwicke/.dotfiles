[user]
	name = Alex Hardwicke
	email = alex.hardwicke@outlook.com
[push]
	default = matching
	followTags = true
[alias]
	st = status
	branch-name = rev-parse --abbrev-ref HEAD
	publish = push -u origin master
    stash-peek = stash show -p stash@{0}
[color "diff"]
	meta = 6
	frag = 3
	old = 1
	new = 2
[color "status"]
	added = 2
	changed = 5
	untracked = 4
[color "branch"]
	current = 4
	local = 6
	remote = 3
[rebase]
    autosquash = true
[stash]
	showPatch = true
[core]
	editor = vim
